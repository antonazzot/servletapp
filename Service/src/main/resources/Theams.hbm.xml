<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name = "by.task.servlets.Model.threadmodel.Theams" table = "theam">

        <id name = "id" type = "int">
            <column name="id"/>
            <generator class="identity"/>
        </id>

        <property name="theamName" column="theam_name"/>

    </class>
</hibernate-mapping>